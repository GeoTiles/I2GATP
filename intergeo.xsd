	<xs:group ref="scalar"/>
	<xs:group ref="scalar"/>
	<xs:group ref="scalar"/>
	<xs:group ref="scalar"/>
	<xs:group ref="scalar"/>
	<xs:group ref="scalar"/>
	<xs:group ref="scalar"/>
	<xs:group ref="scalar"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="complex">
    <xs:complexType>
      <xs:sequence>
	<xs:element ref="double"/>
	<xs:element ref="double"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
